{
  "name": "mvad-electron",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "package-win": "electron-manager . electron-demo --overwrite --asar=true --platform=x64 --arch=amd64 --prune=true"
  },
  "configurations": {
    "name": "mvad-electron",
    "type": "node",
    "request": "launch",
    "program": "${workspaceRoot}/main.js",
    "stopOnEntry": false,
    "args": [],
    "cwd": "${workspaceRoot}",
    "preLaunchTask": null
  },
  "repository": "https://github.com/mvwater/mvwater-electron",
  "author": "skim,mbrydon,scanfield",
  "license": "Apache2",
  "devDependencies": {
    "@angular/cli": "^9.0.6",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "babel": "^6.23.0",
    "babel-loader": "^8.0.6",
    "electron": "^8.2.4",
    "electron-cgi": "^1.0.2",
    "electron-packager": "^13.1.1",
    "gulp-mocha": "^7.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^7.1.0",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "tslint": "^5.20.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "mysql": "^2.18.1"
  }
}
